{
    "name": "Profile Extension",
    "description": "This Extension contains my all social media profile",
    "version": "1.1",
    "manifest_version": 2,

    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",

    },
    "permissions": [
        "activeTab",
        "webNavigation",
        "tabs",
        "storage"
    ]
}